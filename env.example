# Spotify Configuration
# Copy this file to .env and fill in your actual values

# Your Spotify App Client ID (required)
VITE_SPOTIFY_CLIENT_ID=""

# Your Spotify App Client Secret (required for server-side token exchange)
VITE_SPOTIFY_CLIENT_SECRET=""

# Spotify Redirect URI (optional - will auto-detect for development)
VITE_SPOTIFY_REDIRECT_URI=""

# App Configuration
VITE_APP_NAME=Ubiquity
VITE_DEBUG=true
